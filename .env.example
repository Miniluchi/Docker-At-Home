# Configuration générale
TZ=Europe/Paris
PUID=
PGID=

# Homarr
HOMARR_SECRET_ENCRYPTION_KEY=

# Homarr SSO avec Authentik
HOMARR_OIDC_CLIENT_ID=
HOMARR_OIDC_CLIENT_SECRET=
HOMARR_OIDC_SLUG=
HOMARR_OIDC_AUTO_LOGIN=

# Traefik
TRAEFIK_ACME_EMAIL=

# Zigbee
ZIGBEE_DONGLE_PATH=

# Planka
PLANKA_SECRET_KEY=
PLANKA_POSTGRES_USER=
PLANKA_POSTGRES_PASSWORD=
PLANKA_POSTGRES_DB=

# Planka SSO avec Authentik (OIDC)
# Note: Ces valeurs seront générées dans l'interface Authentik
# Créez un provider OAuth2/OpenID avec Redirect URI: https://planka.${DOMAIN_BASE}/oidc-callback
PLANKA_OIDC_CLIENT_ID=
PLANKA_OIDC_CLIENT_SECRET=
PLANKA_OIDC_ADMIN_ROLES=admins

# Domaines
DOMAIN_BASE=

# Watchtower - Notifications Email (OVH SMTP)
WATCHTOWER_NOTIFICATION_EMAIL_FROM=
WATCHTOWER_NOTIFICATION_EMAIL_TO=
WATCHTOWER_NOTIFICATION_EMAIL_SERVER=
WATCHTOWER_NOTIFICATION_EMAIL_SERVER_PORT=
WATCHTOWER_NOTIFICATION_EMAIL_SERVER_USER=
WATCHTOWER_NOTIFICATION_EMAIL_SERVER_PASSWORD=

# Media - Chemin racine pour tous les médias (downloads, films, séries)
MEDIA_PATH=

# Authentik - SSO/Identity Provider
AUTHENTIK_POSTGRES_USER=
AUTHENTIK_POSTGRES_PASSWORD=
AUTHENTIK_POSTGRES_DB=
AUTHENTIK_SECRET_KEY=
AUTHENTIK_BOOTSTRAP_PASSWORD=
AUTHENTIK_BOOTSTRAP_EMAIL=

# Optionnel: version d'Authentik (par défaut: 2025.10.1)
AUTHENTIK_TAG=

# Jellyfin SSO avec Authentik
# Note: Ces valeurs seront générées dans l'interface Authentik
# Créez un provider OAuth2/OpenID avec Redirect URI: https://jellyfin.${DOMAIN_BASE}/sso/OID/redirect/authentik
JELLYFIN_OIDC_CLIENT_ID=
JELLYFIN_OIDC_CLIENT_SECRET=

# Authentik Proxy Outpost - Token pour l'authentification des apps *arr
# Note: Ce token sera généré dans l'interface Authentik après création de l'outpost
# Allez dans Admin > Outposts > [Votre outpost] et copiez le token
AUTHENTIK_PROXY_TOKEN=

# Portainer SSO avec Authentik
PORTAINER_OIDC_CLIENT_ID=
PORTAINER_OIDC_CLIENT_SECRET=
